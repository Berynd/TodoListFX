<VBox alignment="CENTER" prefHeight="471.0" prefWidth="231.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.accueil.InscriptionController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label alignment="TOP_CENTER" text="Inscription">
        <font>
            <Font size="21.0" />
        </font>
    </Label>
    <Label text="nom" />
    <TextField fx:id="nom" />
    <Label text="prenom" />
    <TextField fx:id="prenom" />
   <Label text="email" />
   <TextField fx:id="email" />
   <Label text="mot de passe" />
   <TextField fx:id="mdp" />
   <Label text="confirmation" />
   <TextField fx:id="confirmation" />
    <Button onAction="#onInscription" text="Inscription" />
    <Button mnemonicParsing="false" onAction="#onRetour" text="retour" />
</VBox>
